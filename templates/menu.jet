                opacity: 0;
            }
            20% {
                opacity: 0.3;
            }
            90% {
                opacity: 0.2;
            }
            100% {
                transform: translateY(-20vh) scale(1.2);
                opacity: 0;
            }
        }

        @keyframes drift {
            0% {
                transform: translateX(-10vw);
            }
            100% {
                transform: translateX(10vw);
